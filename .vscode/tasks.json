{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Build",
			"command": "/usr/bin/gcc-11",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-Wall",
				"-o",
				"../obj/${fileBasenameNoExtension}.o",
				"-c",
				"${file}",
				"-I../include"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "Task generated by Debugger."
		}
	],
	"version": "2.0.0"
}
